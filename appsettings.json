{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Azure": "Information",
        "IdentityModel": "Warning",
        "Duende": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.Authentication": "Debug",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "withProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "CalcApi"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*",
  "Auth": {
    "Authority": "https://apisummit.eu.auth0.com/",
    "Audience": "http://calculator-api"
  }
}
